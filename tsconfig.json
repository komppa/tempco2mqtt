    "compilerOptions": {
      "noImplicitAny": true,
      "removeComments": true,
      "allowJs": true,
      "moduleResolution": "node",
      "preserveConstEnums": true,
      "sourceMap": true,
      "outDir": "build"
    },
    "include": ["**/*"],
    "exclude": ["node_modules", "**/*.spec.ts"]
}